                .thumb-wrap {
                        display: inline-block;
                        float: left;
                        width: 138px;
                        height: 178px;
                        overflow: hidden;
                        margin: 8px 16px 0px 0px;
                        padding-top: 8px;
                        background-image: none;
                        position: relative;
                        border: 1px solid transparent;
